<aura:component >
    <aura:attribute name="workItems" type="Work_Item__c[]"/>
    	<aura:iteration items="{!v.workItems}" var="workItem">
            <div class="slds-p-bottom_x-small slds-p-top_x-small 
                        slds-border_left slds-border_bottom slds-border_right
                        slds-border_top slds-p-left_x-small slds-p-right_x-small">
        		<c:workItem workItem="{!workItem}"/>
            </div>
    	</aura:iteration>
</aura:component>